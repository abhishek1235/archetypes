<widget>
    <name>widget-navbar</name>
    <contextItemName>launchpad</contextItemName>
    <extendedItemName>widget-launchpad-base</extendedItemName>
    <properties>
        <property name="title">
            <value type="string">Navigation Bar</value>
        </property>
        <property name="thumbnailUrl">
            <value type="string">$(contextRoot)/static/launchpad/widgets/navbar/icon.png</value>
        </property>
        <property name="src">
            <value type="string">$(contextRoot)/static/launchpad/widgets/navbar/index.html</value>
        </property>
    </properties>
    <tags>
        <tag type="regular">navigation</tag>
        <tag type="section">launchpad</tag>
    </tags>
    <securityProfile>ADMIN</securityProfile>
</widget>