<archetype-descriptor xmlns="http://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.0.0"
                      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                      xsi:schemaLocation="http://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.0.0 http://maven.apache.org/xsd/archetype-descriptor-1.0.0.xsd"
                      name="Backbase Portal Launchpad Archetype">

    <fileSets>
        <fileSet filtered="false" encoding="UTF-8">
            <directory></directory>
            <includes>
                <include>README.md</include>
                <include>pom.xml</include>
            </includes>
        </fileSet>
        <fileSet filtered="true" encoding="UTF-8">
            <directory>parent</directory>
        </fileSet>
        <fileSet filtered="false" encoding="UTF-8">
            <directory>portal/configuration</directory>
        </fileSet>
        <fileSet filtered="false" encoding="UTF-8">
            <directory>portal/tools</directory>
        </fileSet>
        <fileSet filtered="false" encoding="UTF-8">
            <directory>portal/src/test/resources</directory>
        </fileSet>
        <fileSet filtered="true" encoding="UTF-8" packaged="true">
            <directory>portal/src/test/java</directory>
        </fileSet>
        <fileSet filtered="true" encoding="UTF-8" packaged="false">
            <directory>portal/src/main/java</directory>
        </fileSet>
        <fileSet filtered="false" encoding="UTF-8">
            <directory>portal/src/main/webapp/static</directory>
            <includes>
                <include>version.txt</include>
            </includes>
        </fileSet>
        <fileSet filtered="false" encoding="UTF-8">
            <directory>portal/src/main/webapp</directory>
        </fileSet>
        <fileSet filtered="false" encoding="UTF-8">
            <directory>portal/src/main/dashboardResources</directory>
        </fileSet>
        <fileSet filtered="false" encoding="UTF-8">
            <directory>portal/src/main/coreResources</directory>
        </fileSet>
        <fileSet filtered="true" encoding="UTF-8">
            <directory>portal/src/main/resources</directory>
        </fileSet>
        <fileSet filtered="false" encoding="UTF-8">
            <directory>portal/src/main/config-info</directory>
        </fileSet>
        <fileSet filtered="true" encoding="UTF-8">
            <directory>portal</directory>
        </fileSet>
        <fileSet filtered="true" encoding="UTF-8">
            <directory>launchpad-theme</directory>
            <includes>
                <include>pom.xml</include>
            </includes>
        </fileSet>
        <fileSet filtered="false" encoding="UTF-8">
            <directory>launchpad-theme</directory>
        </fileSet>
    </fileSets>
</archetype-descriptor>
